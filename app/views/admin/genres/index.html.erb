
<h2>ジャンル一覧・追加</h2>
  <%= form_with model: @genre, url:'/admin/genres', local:true do |f| %>
    <p>ジャンル名
      <%= f.text_field :name %>
      <%= f.submit '投稿' %>
    </p>
    <% end %>
    

<table>
  <!--<thead>-->
    <tr>
      <th>ジャンル</th>
    </tr>
  <!--</thead>-->
  <!--<tbody>-->
  <% @genres.each do |genre| %>
    <tr>
      <td><%= genre.name %></td>
      <td>編集する</td>
    </tr>
  <% end %>
  <!--</tbody>-->
</table>

  